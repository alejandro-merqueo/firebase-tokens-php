{
    "name": "merqueo/firebase-tokens-5",
    "description": "A library to work with Firebase tokens",
    "type": "library",
    "keywords": ["firebase", "google", "token", "authentication", "auth"],
    "homepage": "https://github.com/alejandro-merqueo/firebase-tokens-php",
    "license": "MIT",
    "authors": [
        {
            "name": "Alejandro Rivera",
            "homepage": "https://github.com/alejandro-merqueo"
        }
    ],
    "require": {
        "php": "^5.0",
        "lcobucci/jwt": "^3.2",
        "fig/http-message-util": "^1.1",
        "guzzlehttp/guzzle": "^6.2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7",
        "friendsofphp/php-cs-fixer": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Firebase\\Auth\\Token\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Firebase\\Auth\\Token\\Tests\\": "tests"
        }
    },
    "config": {
        "platform": {
            "php": "5.6"
        },
        "sort-packages": true
    }
}
